    <!--STYLING-->
    <style name="Dashboard" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/primary_dark</item>
        <item name="colorControlNormal">@android:color/white</item>
        <item name="colorControlHighlight">@color/primary_dark</item>
        <item name="fontFamily">@font/app_font</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <!--Button Theme -->
    <style name="Dashboard.ButtonNormal" parent="Dashboard">
        <item name="colorControlHighlight">@color/primary_dark</item>
        <item name="colorButtonNormal">@color/primary</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
    </style>

    <style name="Dashboard.ButtonInverse" parent="Dashboard">
        <item name="colorControlHighlight">@color/primary_light</item>
        <item name="colorButtonNormal">@color/white</item>
        <item name="android:textColor">@color/primary_text</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
    </style>

    <style name="Dashboard.ButtonBorderLess" parent="Dashboard">
        <item name="colorControlHighlight">@color/accent</item>
        <item name="colorButtonNormal">@color/transparent</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Title</item>
    </style>

    <style name="Dashboard.TextAppearance.Small" parent="Base.TextAppearance.AppCompat.Small">
        <item name="android:fontFamily">@font/app_font</item>
        <item name="fontFamily">@font/app_font</item>
    </style>

    <style name="Dashboard.TextAppearance.Medium" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:fontFamily">@font/app_font</item>
        <item name="fontFamily">@font/app_font</item>
    </style>

    <style name="Dashboard.TextAppearance.Title" parent="Base.TextAppearance.AppCompat.Title">
        <item name="android:fontFamily">@font/app_font</item>
        <item name="fontFamily">@font/app_font</item>
        <item name="android:textColor">@color/primary_dark</item>
    </style>


    <style name="TextViewInterest" parent="Dashboard.TextAppearance.Small">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@drawable/round_text_view_bg_black</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="TextViewNickname" parent="Dashboard.TextAppearance.Medium">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

